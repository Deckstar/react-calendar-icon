{
  "name": "react-calendar-icon",
  "version": "0.9.4",
  "description": "A customizable date badge component in the shape of a familiar calendar icon ",
  "type": "module",
  "main": "dist/react-calendar-icon.cjs.js",
  "module": "lib/index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "babel --extensions \".ts,.tsx\" src -d lib",
    "build:dist": "tsc",
    "lint": "eslint src",
    "lint:watch": "esw -w src/**",
    "test": "npm run test:web && npm run test:size",
    "test:web": "jest",
    "test:size": "bundlesize",
    "prepublish": "npm run build"
  },
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkostov/react-calendar-icon.git"
  },
  "keywords": [
    "react",
    "front-end",
    "calendar",
    "date"
  ],
  "author": "Konstantin Kostov <me@kvkostov.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkostov/react-calendar-icon/issues"
  },
  "homepage": "https://github.com/kkostov/react-calendar-icon#readme",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.2.1"
  },
  "files": [
    "CODE_OF_CONDUCT.md",
    "dist",
    "lib",
    "src"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-test-renderer": "^16.9.5",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.18.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "jest-styled-components": "^7.0.3",
    "react-test-renderer": "^16.14.0",
    "ts-essentials": "^7.0.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.2.3"
  },
  "bundlesize": [
    {
      "path": "./dist/react-calendar-icon.min.js",
      "threshold": "2kB"
    }
  ]
}
